                      <img id="article-image" src="<%= article.image %>" alt="NEWS">
                      
                      <div class="px-6 py-2">
                        
                          <div class="font-bold text-xl mb-2 sm:text-center title underline"><%= article.title %> <img src="assets\images\icons8-link-32.png" class="linkIcon inline-block"></div>
                           
                        
                        
                        
